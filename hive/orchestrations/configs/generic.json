{
  "id": "generic",
  "name": "Generic Campaign Orchestration",
  "description": "Flexible orchestration for various campaign types with configurable agents",
  "orchestrator": "AgentOrchestrator",
  "version": "1.0.0",
  "agents": {
    "research": {
      "class": "ResearchAudienceAgent",
      "model": "gpt-4o-2024-08-06",
      "temperature": 0.2,
      "description": "Research and audience analysis agent"
    },
    "trending": {
      "class": "TrendingNewsAgent",
      "model": "gpt-4o-2024-08-06", 
      "temperature": 0.3,
      "description": "Trending news and cultural analysis agent"
    },
    "story": {
      "class": "StoryAnglesAgent",
      "model": "gpt-4o-2024-08-06",
      "temperature": 0.4,
      "description": "Story angles and headlines generation agent"
    },
    "pr_manager": {
      "class": "PRManagerAgent",
      "model": "gpt-4o-mini-2024-07-18",
      "temperature": 0.2,
      "description": "PR campaign management and coordination agent"
    },
    "strategic_insight": {
      "class": "StrategicInsightAgent",
      "model": "gpt-4o-2024-08-06",
      "temperature": 0.1,
      "description": "Strategic insights and human truth analysis agent"
    },
    "brand_lens": {
      "class": "BrandLensAgent",
      "model": "gpt-4o-2024-08-06",
      "temperature": 0.2,
      "description": "Brand analysis and positioning agent"
    },
    "trend_cultural": {
      "class": "TrendCulturalAnalyzerAgent",
      "model": "gpt-4o-2024-08-06",
      "temperature": 0.3,
      "description": "Cultural trend analysis agent"
    },
    "visual_prompt": {
      "class": "VisualPromptGeneratorAgent",
      "model": "gpt-4o-2024-08-06",
      "temperature": 0.4,
      "description": "Visual prompt generation agent"
    },
    "modular_elements": {
      "class": "ModularElementsRecommenderAgent",
      "model": "gpt-4o-2024-08-06",
      "temperature": 0.2,
      "description": "Modular campaign elements recommender agent"
    },
    "brand_qa": {
      "class": "BrandQAAgent",
      "model": "gpt-4o-2024-08-06",
      "temperature": 0.1,
      "description": "Brand Q&A and consistency agent"
    }
  },
  "workflow": {
    "phases": ["research", "strategic_insight", "trending", "story", "collaborative"],
    "enableManualReview": true,
    "enableDynamicFlow": true,
    "enableQualityControl": true
  },
  "settings": {
    "enableManualReview": true,
    "enableDynamicFlow": true,
    "enableQualityControl": true,
    "enableAgentInteraction": true,
    "requireFinalSignoff": true
  }
} 