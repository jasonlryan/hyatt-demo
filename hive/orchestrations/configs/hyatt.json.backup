{
  "id": "hyatt",
  "name": "Hyatt PR Campaign Orchestration",
  "description": "Specialized orchestration for Hyatt hotel PR campaigns with research, trending analysis, story angles, and strategic insights",
  "orchestrator": "AgentOrchestrator",
  "version": "1.0.0",
  "agents": {
    "research": {
      "class": "ResearchAudienceAgent",
      "model": "gpt-4o-2024-08-06",
      "temperature": 0.2,
      "description": "Research and audience analysis agent"
    },
    "trending": {
      "class": "TrendingNewsAgent", 
      "model": "gpt-4o-2024-08-06",
      "temperature": 0.3,
      "description": "Trending news and cultural analysis agent"
    },
    "story": {
      "class": "StoryAnglesAgent",
      "model": "gpt-4o-2024-08-06", 
      "temperature": 0.4,
      "description": "Story angles and headlines generation agent"
    },
    "pr_manager": {
      "class": "PRManagerAgent",
      "model": "gpt-4o-mini-2024-07-18",
      "temperature": 0.2,
      "description": "PR campaign management and coordination agent"
    },
    "strategic_insight": {
      "class": "StrategicInsightAgent",
      "model": "gpt-4o-2024-08-06",
      "temperature": 0.1,
      "description": "Strategic insights and human truth analysis agent"
    }
  },
  "workflow": {
    "phases": ["research", "strategic_insight", "trending", "story", "collaborative"],
    "enableManualReview": true,
    "enableDynamicFlow": true,
    "enableQualityControl": true
  },
  "settings": {
    "enableManualReview": true,
    "enableDynamicFlow": true,
    "enableQualityControl": true,
    "enableAgentInteraction": true,
    "requireFinalSignoff": true
  }
} 